[configuration]
entry_symbol = "gdext_rust_init"
compatibility_minimum = 4.2
reloadable=true

[libraries]
linux.debug.x86_64 = "res://target/debug/libgodot_json_schema.so"
linux.release.x86_64 = "res://target/release/libgodot_json_schema.so"
windows.debug.x86_64 = "res://target/debug/godot_json_schema.dll"
windows.release.x86_64 = "res://target/release/godot_json_schema.dll"
macos.debug = "res://target/debug/libgodot_json_schema.dylib"
macos.release = "res://target/release/libgodot_json_schema.dylib"
macos.debug.arm64 = "res://target/debug/libgodot_json_schema.dylib"
macos.release.arm64 = "res://target/release/libgodot_json_schema.dylib"
web.debug.wasm32 = "res://target/wasm32-unknown-emscripten/debug/godot_json_schema.wasm"
web.release.wasm32 = "res://target/wasm32-unknown-emscripten/release/godot_json_schema.wasm"